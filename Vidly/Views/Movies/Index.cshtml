@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Peliculas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Peliculas</h2>
<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Pelicula</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>

